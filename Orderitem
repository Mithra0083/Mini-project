package Model;

import java.util.ArrayList; import java.util.List;

public class Orderitem { private List<FoodItem> items = new ArrayList<>();

public void addItem(FoodItem item) { items.add(item); }

public List<FoodItem> getItems() { return items; }

public double calculateTotal() { double total = 0; for (FoodItem item: items) { total += item.getPrice(); }

return total;
 }
}
